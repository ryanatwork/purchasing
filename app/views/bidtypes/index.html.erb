<h1>Listing bidtypes</h1>

<table>
  <tr>
    <th>Abbreviation</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bidtypes.each do |bidtype| %>
  <tr>
    <td><%= bidtype.abbreviation %></td>
    <td><%= bidtype.description %></td>
    <td><%= link_to 'Show', bidtype %></td>
    <td><%= link_to 'Edit', edit_bidtype_path(bidtype) %></td>
    <td><%= link_to 'Destroy', bidtype, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bidtype', new_bidtype_path %>
